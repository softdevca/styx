---
// Top level comment
meta {id "test@1"}

/// Schema documentation
schema {
    @ @object{
        /// A string field
        name @string

        /// An enum with variants
        kind @enum{
            /// Simple kind
            simple @unit
            /// Complex kind
            complex @object{
                /// Nested value
                value @optional(@int)
            }
        }

        /// A sequence
        items @seq(@string)

        /// A map
        data @map(@string@object{x @int, y @int})
    }
}
