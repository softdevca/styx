{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "styx",
  "rules": {
    "document": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_newline"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "doc_comment"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "entry"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "SYMBOL",
                  "name": "_newline"
                }
              }
            ]
          }
        }
      ]
    },
    "entry": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SYMBOL",
          "name": "value"
        }
      }
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC_RIGHT",
          "value": 0,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "FIELD",
                "name": "tag",
                "content": {
                  "type": "SYMBOL",
                  "name": "tag"
                }
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "FIELD",
                    "name": "payload",
                    "content": {
                      "type": "SYMBOL",
                      "name": "_payload"
                    }
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "FIELD",
          "name": "payload",
          "content": {
            "type": "SYMBOL",
            "name": "_payload"
          }
        },
        {
          "type": "SYMBOL",
          "name": "attributes"
        }
      ]
    },
    "tag": {
      "type": "SYMBOL",
      "name": "_tag_start"
    },
    "_payload": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "scalar"
        },
        {
          "type": "SYMBOL",
          "name": "sequence"
        },
        {
          "type": "SYMBOL",
          "name": "object"
        },
        {
          "type": "SYMBOL",
          "name": "unit"
        }
      ]
    },
    "scalar": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bare_scalar"
        },
        {
          "type": "SYMBOL",
          "name": "quoted_scalar"
        },
        {
          "type": "SYMBOL",
          "name": "raw_scalar"
        },
        {
          "type": "SYMBOL",
          "name": "heredoc"
        }
      ]
    },
    "bare_scalar": {
      "type": "PATTERN",
      "value": "[^{}\\(\\),\"=@\\s\\/][^{}\\(\\),\"=@\\s]*"
    },
    "quoted_scalar": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "escape_sequence"
              },
              {
                "type": "PATTERN",
                "value": "[^\"\\\\]+"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "escape_sequence": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "\\\\[\\\\\\\"nrt0]"
          },
          {
            "type": "PATTERN",
            "value": "\\\\u[0-9A-Fa-f]{4}"
          },
          {
            "type": "PATTERN",
            "value": "\\\\u\\{[0-9A-Fa-f]{1,6}\\}"
          }
        ]
      }
    },
    "raw_scalar": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_raw_string_start"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_raw_string_content"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_raw_string_end"
        }
      ]
    },
    "heredoc": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_heredoc_start"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "SYMBOL",
                "name": "_heredoc_lang"
              },
              "named": true,
              "value": "heredoc_lang"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "ALIAS",
              "content": {
                "type": "SYMBOL",
                "name": "_heredoc_content"
              },
              "named": true,
              "value": "heredoc_content"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_heredoc_end"
        }
      ]
    },
    "unit": {
      "type": "SYMBOL",
      "name": "_unit_at"
    },
    "sequence": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "value"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_ws"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "object": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_object_body"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_object_body": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_newline_separated"
        },
        {
          "type": "SYMBOL",
          "name": "_comma_separated"
        }
      ]
    },
    "_newline_separated": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_newline"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "doc_comment"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "entry"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_newline"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "doc_comment"
                  },
                  {
                    "type": "BLANK"
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "entry"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_newline"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_comma_separated": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "entry"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "entry"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "attributes": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "attribute"
      }
    },
    "attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "bare_scalar"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "_attribute_value"
          }
        }
      ]
    },
    "_attribute_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bare_scalar"
        },
        {
          "type": "SYMBOL",
          "name": "quoted_scalar"
        },
        {
          "type": "SYMBOL",
          "name": "sequence"
        },
        {
          "type": "SYMBOL",
          "name": "object"
        }
      ]
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": "[^\\/]"
          },
          {
            "type": "PATTERN",
            "value": "[^\\n\\r]*"
          }
        ]
      }
    },
    "doc_comment": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "///"
          },
          {
            "type": "PATTERN",
            "value": "[^\\n\\r]*"
          },
          {
            "type": "SYMBOL",
            "name": "_newline"
          }
        ]
      }
    },
    "_ws": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]"
    },
    {
      "type": "SYMBOL",
      "name": "line_comment"
    }
  ],
  "conflicts": [
    [
      "attributes"
    ],
    [
      "_newline_separated",
      "_comma_separated"
    ]
  ],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_heredoc_start"
    },
    {
      "type": "SYMBOL",
      "name": "_heredoc_lang"
    },
    {
      "type": "SYMBOL",
      "name": "_heredoc_content"
    },
    {
      "type": "SYMBOL",
      "name": "_heredoc_end"
    },
    {
      "type": "SYMBOL",
      "name": "_raw_string_start"
    },
    {
      "type": "SYMBOL",
      "name": "_raw_string_content"
    },
    {
      "type": "SYMBOL",
      "name": "_raw_string_end"
    },
    {
      "type": "SYMBOL",
      "name": "_unit_at"
    },
    {
      "type": "SYMBOL",
      "name": "_tag_start"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}